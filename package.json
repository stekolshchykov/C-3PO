    "lodash": "^4.17.21",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.16.0",
    "reverso-api": "^2.22.6",
